--飞天

function SJ()
  gg.setRanges(32)
  local dataType = 16
  local tb1 = {{-60.0,0X0},{60.0,0x4},{10.0,0xc},{5.0,0x18},}
  local tb3 = {}
  local tb2 = {
  {-120,0x0,["Ty"]=F},{120,0x4,["Ty"]=F},{-120,0x80,["Ty"]=F},{120,0x84,["Ty"]=F}
  }
  SearchWrite(tb1,tb2,dataType,tb3)
  end
  
  
function FT()
  shu = gg.prompt({'视角飞天高度(建议用这个)','实体飞天高度'},{'',''},{"number","number"})
  if shu == nil then YX()
  else
  ftj1 = shu[2]
  ftj2 = shu[1]
  if ftj1~='' and ftj2~='' then gg.alert("不要两个输入框都填") ftj1='' ftj2='' FT() end
  if SJPD==true then SJ() end
  if ftj1 ~= '' then ftj1=ftj1*0.1*10 ft1() end
  if ftj2 ~= '' then ftj2=ftj2*0.1*10 FT2() end
  end
end


function ft1()
  gg.setRanges(32)
  local dataType = 16
  local tb1 = {{0.57465088367,0},{1.5253000259399414,-0x90},{1.0,-0xd8},{1.0,-0x10},}
  local tb3 = {}
  local tb2 = {
  {ftj1,-0xc0,2,"飞天高度",["Ty"]=F},
  }
  SearchWrite(tb1,tb2,dataType,tb3)
end

function FT2()
gg.setVisible(false)
 ft1 = 0
 fttz=0
 while ft1==0 do
 FT2a = gg.isVisible()
 FT2A()
 gg.sleep(500)
 end
 end

function FT2A()
      local a = gg.getListItems()
      ft2c = false
     for i, item in ipairs(a) do
        if item.name == "视角高度" then
           ft2a = item.value
           ft2b = item.address
           ft2c = true
           break
        end
    end
    if ft2a ~= ftj2 then FT2B() end
    
    if FT2a == true then 
     ft1=1  gg.setVisible(true) FT2B() ft2c=true end
    if not ft2c then 
     ft2() SJ() end
 end 

function FT2B()
    gg.removeListItems({address=ft2b})
end

function ft2()
 gg.setRanges(32)
  local dataType = 16
  local tb1 = {{0.57465088367,0},{1.5253000259399414,-0x90},{1.0,-0x18},{1.0,-0x48},{0.0,-0x28}}
  local tb3 = {}
  local tb2 = {
  {ftj2,0x0,1,"视角高度",["Ty"]=F},
  }
  SearchWrite(tb1,tb2,dataType,tb3)
  end


