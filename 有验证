function csi1()
    iux = io.open("/storage/emulated/0/账号1")
    if not iux then
    zhang1="(空存档)"
    else
    zhang1="存档1"
    end
    iux = io.open("/storage/emulated/0/账号2")
    if not iux then
    zhang2="(空存档)"
    else
    zhang2="存档2"
    end
    iux = io.open("/storage/emulated/0/账号3")
    if not iux then
    zhang3="(空存档)"
    else
    zhang3="存档3"
    end
end


