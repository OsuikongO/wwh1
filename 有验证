--毒气弹
dq1=1
dq2=1
function DQ()
  shu = gg.prompt({'毒气弹模型大小推荐30','毒气弹伤害大小推荐30'},{dq1,dq2})
  if shu == nil then
  YX()
  else
  dq1 = shu[1]
  dq2 = shu[2]
  DQ1()
  end
end

