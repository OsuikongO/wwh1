function gq4a()
   print(found)
    local a = gg.getListItems()
    for i, item in ipairs(a) do
        if item.name == "武器库枪值" then
            found = 3
            gq4a1 = item.value
            if gq4a1==0 and gq4b<=3 then gq4a() gq4b=gq4b+1 end
            break
        end
    end
    if found==0 then found=1 GQ4B()  end
    if found==1 then found=2 GQ4C()  end
    if found==2 then 
       found=3  gg.alert("未找到 请手动输入枪值")  gq4a1 = '90'
    end
end


