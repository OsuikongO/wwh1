function GQ3()
ss = "800"
sh = "200"
jl = "40"
dw = "1"
hzl = ''

  shu = gg.prompt({'枪值','霰弹枪请勾上(自动修改伤害和弹丸)','每分钟开火次数','伤害(自动计算伤害衰减)','距离m(自动计算距衰)','散射角度(不填就是默认)'},{gq4a1,false,ss,sh,jl,hzl},{"number","checkbox"})
  if shu == nil then GQ()
  else
  gq4a1 = shu[1]
  xd=shu[2] 
  ss=shu[3]  ss1=ss*0.1*10.0  ss1a=60.0/ss1 
  sh=shu[4]  sh1=sh*0.1*10.0  sh1a=sh1/2.0
  jl =shu[5]  jl1=jl*0.1*10.0  jl1a=jl1*2.0
  hzl =shu[6]
  if xd==true then 
  sh1=sh1/8.0 sh1a=sh1a/8.0 dw=8 end
  if hzl=='' then gq31()
  else
  hzl1 = hzl*0.1*10.0 hzl2=0 gq3()
  end
  end
end


