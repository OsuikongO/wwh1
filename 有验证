function chundang()
  local t = gg.choice({
  zhang1,
  zhang2,
  zhang3,
  "清除游戏存档"
  }, nil, "")
  if t == nil then Main1() end
  if t==1 then dzizhi1() end
  if t==2 then dzizhi2() end
  if t==3 then dzizhi3() end
  if t==4 then qingyou() end
end

zhang1 = ""
zhang2 = ""
zhang3 = ""

function cundann()
iuix = io.open("/storage/emulated/0/data")
if not iuix then

gg.alert("存档脚本初始化，联系管理(只需进行一次)")

else
chundang()
end
end






D=gg.TYPE_DWORD E=gg.TYPE_DOUBLE F=gg.TYPE_FLOAT W=gg.TYPE_WORD B=gg.TYPE_BYTE X=gg.TYPE_XOR Q=gg.TYPE_QWORD 
function SearchWrite(Search,Write,Type,Write1)
 gg.clearResults()  
 gg.setVisible(false) 
 gg.searchNumber(Search[1][1],Type) 
 local count = gg.getResultCount() 
 local result = gg.getResults(count) 
 gg.clearResults()
 local data = {} 
 local base = Search[1][2]
 if (count > 0) then 
    for i, v in ipairs(result) do 
       v.isUseful = true 
    end 
    for k=2, #Search do 
       local tmp = {} 
       local offset = Search[k][2] - base 
       local num = Search[k][1] 
       for i, v in ipairs(result) do 
          local flags = Search[k][3]
          if not flags then flags=v.flags end
          tmp[#tmp+1] = {} 
          tmp[#tmp].address = v.address + offset 
          tmp[#tmp].flags = flags
       end 
       tmp = gg.getValues(tmp) 
       for i, v in ipairs(tmp) do 
          if ( tostring(v.value) ~= tostring(num) ) then 
             result[i].isUseful = false 
          end 
       end 
    end 
    for i, v in ipairs(result) do 
       if (v.isUseful) then 
          data[#data+1] = v.address 
       end 
    end 
    if (#data > 0) then 
       gg.toast("搜到"..#data.."条数据") 
       local t = {}  
       local base = Search[1][2] 
       for i=1, #data do 
          for k, w in ipairs(Write) do 
             offset = w[2] - base 
             t[#t+1] = {} 
             t[#t].address = data[i] + offset 
             t[#t].flags = w["Ty"] 
             t[#t].value = w[1]  
             t[#t].name = w[4]   
             if (w[3] == 1) then 
                local item = {} 
                item[#item+1] = t[#t] 
                item[#item].freeze = false 
                gg.addListItems(item)  
             end 
             if (w[3] == 2) then 
                local item = {} 
                item[#item+1] = t[#t] 
                item[#item].freeze = true 
                gg.addListItems(item)  
             end 
          end        
          gg.setValues(t) 
       end     
       for i=1, #data do 
          for k, w in ipairs(Write1) do 
             offset = w[2] - base 
             t[#t+1] = {} 
             t[#t].address = data[i] + offset 
             t[#t].flags = w["Ty"] 
             t[#t].name = w[4]  
             if w['get'] == true then tzm = t[#t] end
             if (w[3] == 1) then 
                local item = {} 
                item[#item+1] = t[#t] 
                gg.addListItems(item)  
             end 
             if (w[3] == 2) then 
                local item = {} 
                item[#item+1] = t[#t] 
                item[#item].freeze = true 
                gg.addListItems(item)  
             end 
          end 
       end 
    else gg.toast("ok", false)  
       return false 
    end 
 else 
    gg.toast("ok") 
    return false 
 end 
end 

function GQ()
  local t = gg.choice({
  "简易改枪模式",
  "变态模式",
  "调试模式",
  "改枪皮",
  "寻找枪值",
  }, nil, "推荐使用简易 寻找找枪皮打开仓库选新手霰弹枪")
  if t == nil then Main() end
  if t==1 then GQ4A()  GQ3() end
  if t==2 then GQ4A()  GQ2() end
  if t==3 then GQ4A()  GQ1() end
  if t==4 then GQ0() end
  if t==5 then GQ4A() end
  end
 
 
 gq0a1 = "9"
 gq0b1 = "90"
 function GQ0()
  shu = gg.prompt({'原枪','改枪'},{gq0a1,gq0b1},{"number","number"})
  if shu == nil then GQ()
  else  
  gq0a1 = shu[1]
  gq0a2 = shu[2]
  gq0a() gq0b() gq0()
 end
end


function gq0()
  gg.setRanges(32)
  local dataType = 4
  local tb1 = {{gq0a1,0},{0,0x4},{0,0x78},{1,0x7c},{1,0x98},{1,0xac},{0,0xc0},{1,0xc4},}
  local tb3 = {}
  local tb2 = {{gq0b1,0x88,1,'原枪枪值',["Ty"]=D}}
  SearchWrite(tb1,tb2,dataType,tb3)
end

function gq0a()
  gg.setRanges(32)
  local dataType = 4
  local tb1 = {{gq0a2,0},{0,0x4},{0,0x78},{1,0x7c},{1,0x98},{1,0xac},{0,0xc0},{1,0xc4},}
  local tb3 = {{'result',0x88,1,'要改的枪值(粘贴到原枪值)',["Ty"]=D}}
  local tb2 = {}
  SearchWrite(tb1,tb2,dataType,tb3)
  end

function gq0b()
    local a = gg.getListItems()
    local found = false

    for i, item in ipairs(a) do
        if item.name == "要改的枪值(粘贴到原枪值)" then
            found = true
            gq0b1 = item.value
            break
        end
    end

    if not found then
        gg.alert("未找到名为'原枪枪值'的项。请检查保存列表是否正确。")
    end
end

