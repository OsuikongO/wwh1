function Main1()
  local t = gg.choice({
  "主功能",
  "存档"
  }, nil, "")
  if t == nil then end
  if t==1 then fingongn() end
  if t==2 then csi1() cundann() end
end


function Main()
  local t = gg.choice({
  "改枪",
  "皮肤",
  "角色",
  "技能",
  "坦克",
  "外挂",
  }, nil, "Q群895618879")
  if t == nil then  end
  if t==1 then GQ() end
  if t==2 then PF() end
  if t==3 then RW() end
  if t==4 then JN() end
  if t==5 then tk() end
  if t==6 then YX() end
  
end



----------改坦克
function tk()
  local t = gg.choice({
  "t34",
  "虎式",
  "谢尔曼",
  "吉普",
 }, nil, "上载具之后选你坐的载具 小概率闪退")
  if t == nil then Main() end
  if t==1 then t341() pao() jiqiang1()end
  if t==2 then hushi1() pao() jiqiang1()end
  if t==3 then xieerman1() pao() jiqiang1()end
  if t==4 then jipu1() jiqiang2() end
end


function t341()--t34
  gg.setRanges(32)
  local dataType = 4
  local tb1 = {{1136525312,0},{-1092196762,-0x18},{1058642330,-0x1c},{-1130113270,-0x20},{1088421888,-0x24},{1189918720,-0x2c},}
  local tb3 = {}
  local tb2 = {
  {6666666,0,1,"坦克血量",["Ty"]=F},
  }
  SearchWrite(tb1,tb2,dataType,tb3)
end
 

