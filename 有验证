----------外挂
function YX()
  local t = gg.choice({
  "范围（进游戏前开自动循环30秒 有时候敌人死之后才生效）",
  "飞天",
  "自由视角(看脚底)",
  "强辅瞄",
  "全屏毒气",
  "全局变速",
  }, nil, "")
  if t == nil then Main() end
  if t==1 then FW2() end
  if t==2 then FT() end
  if t==3 then SJ() end
  if t==4 then fm1() end
  if t==5 then DQ() end
  if t==6 then BS() end
end
--范围
function FW1()
  gg.setRanges(32)
  local dataType = 4
  local tb1 = {{1041865114,0x0},{0,0x4},{1041865114,0x8},{1025758986,0xc},{1028443341,-0x18},{33,0x10}}
  local tb3 = {}
  local tb2 = {
  {shuing,0x0,"范围",["Ty"]=F},
  }
  SearchWrite(tb1,tb2,dataType,tb3)
  end

  
yuning = "0.15"
shuing = "0.15"

function FW2()
 shu = gg.prompt({'默认0.15'},{shuing},{"number"})
  if shu == nil then
  YX()
  else
  shuing = shu[1]
  k=1
  while k<=7 do
  gg.sleep(5000)
  FW1()
  k=k+1
   end
  end
end

function fan1()
  if shu[1] == '' then
    YX()
  else
  gg.setRanges(32)
  local dataType = 16
  local tb1 = {{yuning,0},{0.15,0x8},{0.04,0xc},{4.62428493e-44,0x10},{0.05,-0x18},{9.21942286e-41,-0x14}}
  local tb3 = {}
  local tb2 = {
  {shuing,0x0,["Ty"]=F},
  }
  SearchWrite(tb1,tb2,dataType,tb3)
  yuning = shuing
  end
end



