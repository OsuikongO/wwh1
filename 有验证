function JN()
 local t = gg.choice({
  "喷火器",
  "喷火器改硫酸",
  "喷火器改狗（持续时间0 刷战队积分）",
  "喷火器改炮台（垃圾）",
  "喷火器改侦查",
  }, nil, "（狗 机枪 炮台只能改一次 想换只能重启游戏）")
  if t==nil then Main() end
  if t==1 then jna() JN1() end
  if t==2 then jna() JN2() end
  if t==3 then jna() JN3() end
  if t==4 then jna() JN4() end
  if t==5 then jna() JN5() end
  end
 
function jna()
 shu=gg.prompt({'冷却时间','持续时间(修改后想换只能重启游戏)','狗击杀多少后消失(仅狗生效)'},{0,30,2},{'number'})
 if shu==nil then JN() else
 jna1=shu[1]
 cxsj=shu[2]
 JN3K=shu[3]
 end
 end
 
function JN1()
  gg.setRanges(32)
  local dataType = 16
  local tb1 = {{30,0},{3,0x18,D},{2,0x8,D},{1,0x10,D},{1,0x14,D}}
  local tb3 = {}
  local tb2 = {{cxsj,0x0,['Ty']=F},{jna1,0x4,["Ty"]=F}}
  SearchWrite(tb1,tb2,dataType,tb3)
end

function JN1A()
  gg.setRanges(32)
  local dataType = 16
  local tb1 = {{30,0},{3,0x18,D},{2,0x8,D},{1,0x10,D},{1,0x14,D}}
  local tb3 = {}
  local tb2 = {{cxsj,0x0,['Ty']=F},{jna1,0x4,["Ty"]=F},{jna2,-0x14,['Ty']=D},{jna3,0xc,['Ty']=D}}
  SearchWrite(tb1,tb2,dataType,tb3)
end

function JN2()
  gg.setRanges(32)
  local dataType = 4
  local tb1 = {{224,0},{4,0xc},{2,-0x4},{1,0x4},{1,0x8}}
  local tb3 = {{1,0x0,["Ty"]=D,["get"]=true}}
  local tb2 = {}
  SearchWrite(tb1,tb2,dataType,tb3)
  local a = gg.getValues({{address=tzm.address-0x20,flags=4,value=0}})
  local b = gg.getValues({{address=tzm.address,flags=4,value=0}})
  jna2=a[1].value
  jna3=b[1].value
  JN1A()
end


