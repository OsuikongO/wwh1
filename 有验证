--皮肤
function PF()
  local t = gg.choice({
  "m3",
  "默认喷子",
  "默认狙击",
  "默认机枪",
  "默认手枪",
  }, nil, "")
  if t == nil then Main() end
  if t==1 then PF1() end
  if t==2 then PF2() end
  if t==3 then PF3() end
  if t==4 then PF4() end
  if t==5 then PF5() end
end

function PF1()
gg.setRanges(32)
  local dataType = 4
  local tb1 = {{29,0},{0,0x180},{0,0x19c},{0,0x1a0},{0,0x1a4},{0,0x1a8},{0,0x1cc},{0,0x1d0},{9,0x1C0},}
  local tb3 = {}
