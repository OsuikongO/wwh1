function dzizhi1()
  local t = gg.choice({
  "存档",
  "读取",
  "删除"
  }, nil, "")
  if t == nil then end
  if t==1 then xieru1() end
  if t==2 then duqu1() end
  if t==3 then shanchu1()end
  csi1()
  chundang()
end

